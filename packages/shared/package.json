{
  "name": "@apolitical-assistant/shared",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./keychain": {
      "types": "./dist/keychain.d.ts",
      "import": "./dist/keychain.js",
      "default": "./dist/keychain.js"
    },
    "./notify": {
      "types": "./dist/notify.d.ts",
      "import": "./dist/notify.js",
      "default": "./dist/notify.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  }
}
